{
    "env": {
        "browser": true,
        "es2021": true
    },
    
    "extends": [
        "plugin:react/recommended",
        "airbnb",
        "prettier"    
    ],
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
        "react",
        "prettier"
    ],
    "rules": {
        "prettier/prettier": "warn",
        "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
        "react/state-in-constructor": "off",
        "react/prop-types": "off"
    }
    
}
